# Included by CMakeLists, edited by the build script
# (the version numbers are generated by the build script from the git current label)

set(SLIC3R_APP_NAME "ElegooSlicer")
set(SLIC3R_APP_KEY "ElegooSlicer")

if(NOT DEFINED BBL_RELEASE_TO_PUBLIC)
set(BBL_RELEASE_TO_PUBLIC "1")
endif()
if(NOT DEFINED BBL_INTERNAL_TESTING)
set(BBL_INTERNAL_TESTING "0")
endif()

if(NOT DEFINED BUILD_NUMBER)
    set(BUILD_NUMBER "0")
endif()

set(ELEGOOSLICER_SHORT_VERSION "1.1.8")
set(ELEGOOSLICER_VERSION "${ELEGOOSLICER_SHORT_VERSION}.${BUILD_NUMBER}")
string(REGEX MATCH "^([0-9]+)\\.([0-9]+)\\.([0-9]+)"
       ELEGOOSLICER_VERSION_MATCH ${ELEGOOSLICER_VERSION})
set(ELEGOO_VERSION_MAJOR ${CMAKE_MATCH_1})
set(ELEGOO_VERSION_MINOR ${CMAKE_MATCH_2})
set(ELEGOO_VERSION_PATCH ${CMAKE_MATCH_3})
set(ELEGOO_VERSION_TWEAK ${BUILD_NUMBER})

set(SLIC3R_VERSION ${ELEGOOSLICER_VERSION})
set(SLIC3R_BUILD_ID "${ELEGOOSLICER_VERSION}")

# The forked version of Orca, used for some compatibility
set(ORCA_VERSION "2.2.0")
